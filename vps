apt-get update && apt-get upgrade && apt-get install nano && apt-get install build-essential && wget -c https://github.com/raboof/nethogs/archive/v0.8.5.tar.gz && tar xf v0.8.5.tar.gz && cd ./nethogs-0.8.5/ && sudo apt-get install libncurses5-dev libpcap-dev && make && sudo make install && cd && apt-get install nload && apt-get install htop && apt-get install python-pip && pip install speedtest-cli && wget https://github.com/extremetips/net-free/blob/master/install2.sh && bash install2.sh && menu